{
	"name": "nick-vscode-extension",
	"displayName": "Nick's VS Code Extension",
	"description": "My VS Code Extension",
	"categories": [
		"Themes"
	],
	"version": "0.1.3",
	"publisher": "nick-ng",
	"repository": {
		"type": "git",
		"url": "https://github.com/nick-ng/vscode-extension.git"
	},
	"icon": "/icons/nick_one.png",
	"license": "MIT",
	"engines": {
		"vscode": "^1.0.0"
	},
	"main": "js/main.cjs",
	"activationEvents": [
		"onStartupFinished"
	],
	"scripts": {
		"start": "npm run package",
		"package": "vsce package"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.nickToggleUseIgnoreFiles",
				"title": "Toggle Use Ignore Files"
			}
		],
		"iconThemes": [
			{
				"id": "vs-nick",
				"label": "Nick Icons",
				"path": "./icons/nick-icon-theme.json"
			}
		]
	},
	"devDependencies": {
		"@vscode/vsce": "^2.19.0"
	}
}
